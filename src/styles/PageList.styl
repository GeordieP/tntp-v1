#pageList
    display: flex
    justify-content: center
    align-items: center
    align-content: center
    flex-wrap: wrap
    width: 80vw

#pageList li
    z-index: 1
    border-radius: 2px
    margin: 4px
    width: 20vh
    height: @width
    opacity: 0.4
    transition: opacity 0.2s ease
    &:hover
        opacity: 1
